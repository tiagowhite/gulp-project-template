{
  "port": "9000",
  "notify": false,
  "base_dir": "./app",
  "src_folder": [
    "./app/**/*",
    "!./app/img/original",
    "!./app/templates{,/**}",
    "!./app/data{,/**}"
  ],
  "dist_folder": "./dist/",
  "del_folder": "./dist",
  "templates_src": "./app/**/*.pug",
  "templates_dest": "./dist/",
  "fonts_src": "./app/fonts/*",
  "fonts_dest": "./dist/fonts",
  "sass_src": "./app/css/**/*.scss",
  "sass_dest": "./dist/css",
  "css_file_name": "app.min.css",
  "js_src": [
    "./app/js/*.js",
    "./app/js/vendor/**/*.js",
    "./app/js/build/**/*.js"
  ],
  "js_build_src": "./app/js/build/**/*.js",
  "js_vendor_src": "./app/js/vendor/**/*.js",
  "js_file_name": "app.min.js",
  "js_file_name_vendor": "vendor.min.js",
  "js_dest": "./dist/js",
  "js_vendor_dest": "./dist/js/vendor",
  "img_src": "./app/img/**/*",
  "img_dest": "./dist/img",
  "data_src": "./app/data/**/*",
  "data_dest": "./dist/data",
  "video_src": "./app/videos/**/*",
  "video_dest": "./dist/videos"
}
